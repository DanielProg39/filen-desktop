{
	"name": "filen-desktop",
	"version": "2.0.1",
	"private": true,
	"engines": {
		"node": "14.x"
	},
	"dependencies": {
		"@chakra-ui/react": "^1.8.7",
		"@emotion/react": "^11.8.2",
		"@emotion/styled": "^11.8.1",
		"@gerhobbelt/gitignore-parser": "^0.2.0-9",
		"@testing-library/jest-dom": "^5.16.3",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"@types/jest": "^28.1.3",
		"@types/node": "^18.0.0",
		"@types/react": "^18.0.14",
		"@types/react-dom": "^18.0.5",
		"@types/uuid": "^8.3.4",
		"@uiw/react-codemirror": "^4.7.0",
		"auto-launch": "^5.0.5",
		"cross-env": "^7.0.3",
		"crypto-js": "^4.1.1",
		"electron-disable-file-drop": "^1.0.2",
		"electron-is": "^3.0.0",
		"electron-is-dev": "^1.2.0",
		"electron-log": "^4.4.6",
		"electron-traywindow-positioner": "^1.1.1",
		"electron-updater": "^5.0.1",
		"framer-motion": "^3.10.6",
		"fs-extra": "^10.1.0",
		"http": "^0.0.1-security",
		"https": "^1.0.0",
		"js-base64": "^3.7.2",
		"js-md2": "^0.2.2",
		"js-md4": "^0.3.2",
		"js-md5": "^0.7.3",
		"js-sha1": "^0.6.0",
		"js-sha256": "^0.9.0",
		"js-sha512": "^0.8.0",
		"level": "^8.0.0",
		"lodash": "^4.17.21",
		"mime-types": "^2.1.35",
		"node-watch": "^0.7.3",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-fast-compare": "^3.2.0",
		"react-icons": "^4.3.1",
		"react-router-dom": "^6.3.0",
		"react-scripts": "5.0.0",
		"react-virtualized": "^9.22.3",
		"read-chunk": "^4.0.2",
		"readdirp": "^3.6.0",
		"request": "^2.88.2",
		"socket.io-client": "^2.4.0",
		"speed-limiter": "^1.0.2",
		"typescript": "^4.7.4",
		"uuid": "^8.3.2"
	},
	"scripts": {
		"start": "concurrently -k \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electronmon .\"",
		"dev": "concurrently -k \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electronmon .\"",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"dist": "rm -rf dist/ && react-scripts build && electron-builder -wlm",
		"build-linux": "rm -rf dist/ && react-scripts build && electron-builder -l",
		"build-mac": "rm -rf dist/ && react-scripts build && electron-builder -m",
		"build-win": "rm -rf dist/ && react-scripts build && electron-builder -w",
		"build-win-win": "react-scripts build && electron-builder -w",
		"react-start": "react-scripts start",
		"rebuild": "electron-rebuild",
		"rebuild:arm64": "electron-rebuild --arch arm64",
		"postinstall": "electron-builder install-app-deps",
		"icons": "electron-icon-builder --input=./assets/icon.png --output=./assets/"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"electron": "^19.0.7",
		"electron-builder": "^23.1.0",
		"electron-icon-builder": "^2.0.1",
		"electron-notarize": "^1.2.1",
		"electron-rebuild": "^3.2.7",
		"electronmon": "^1.1.2",
		"wait-on": "^5.3.0"
	},
	"homepage": "./",
	"main": "./src/main/index.js",
	"build": {
		"appId": "io.filen.desktop",
		"productName": "Filen",
		"asar": false,
		"npmRebuild": true,
		"nodeGypRebuild": false,
		"extends": null,
		"artifactName": "filen_${arch}.${ext}",
		"afterSign": "./scripts/notarize.js",
		"files": [
			"build",
			"node_modules",
			"src",
			"package.json"
		],
		"directories": {
			"output": "./dist/",
			"buildResources": "assets"
		},
		"publish": [
			{
				"provider": "generic",
				"url": "https://cdn.filen.io/desktop/release/"
			}
		],
		"win": {
			"icon": "./assets/icons/win/icon.ico",
			"target": {
				"target": "nsis",
				"arch": [
					"x64",
					"arm64"
				]
			}
		},
		"nsis": {
			"oneClick": false,
			"perMachine": false,
			"allowElevation": true,
			"createDesktopShortcut": true,
			"createStartMenuShortcut": true,
			"deleteAppDataOnUninstall": true,
			"allowToChangeInstallationDirectory": false
		},
		"linux": {
			"icon": "./assets/icons/png/512x512.png",
			"target": {
				"target": "AppImage",
				"arch": [
					"x64",
					"arm64"
				]
			}
		},
		"mac": {
			"hardenedRuntime": true,
			"gatekeeperAssess": false,
			"entitlements": "./assets/entitlements.mac.plist",
			"entitlementsInherit": "./assets/entitlements.mac.plist",
			"target": {
				"target": "dmg",
				"arch": [
					"x64",
					"arm64"
				]
			},
			"extendInfo": {
				"LSUIElement": 1
			},
			"icon": "./assets/icons/mac/icon.icns"
		},
		"dmg": {
			"sign": false
		}
	}
}
